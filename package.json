{
  "name": "openui5-basic-template-app",
  "version": "0.1.0",
  "description": "Best-practice starting point for building a freestyle app",
  "private": true,
  "scripts": {
    "start": "ui5 serve",
    "dist": "npm run build && ui5 serve --config ui5-dist.yaml",
    "lint": "eslint webapp",
    "build": "rimraf dist && ui5 build --a"
  },
  "dependencies": {
    "@openui5/sap.m": "^1.78.1",
    "@openui5/sap.f": "^1.78.1",
    "@openui5/sap.ui.core": "^1.78.1",
    "@openui5/sap.ui.integration": "^1.78.1",
    "@openui5/sap.ui.layout": "^1.78.1",
    "@openui5/themelib_sap_fiori_3": "^1.78.1",
    "@sapui5/sap.suite.ui.microchart": "^1.78.1",
    "@sapui5/sap.viz": "^1.78.1"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "@ui5/cli": "^2.2.0",
    "ui5-middleware-cfdestination": "^0.1.8",
    "ui5-middleware-livereload": "^0.2.0",
    "ui5-middleware-simpleproxy": "^0.2.3"
  },
  "ui5": {
    "dependencies": [
      "ui5-middleware-simpleproxy",
      "ui5-middleware-cfdestination",
      "ui5-middleware-livereload"
    ]
  }
}
